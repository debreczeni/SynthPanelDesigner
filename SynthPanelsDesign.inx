                    <param name="slider_scale_label_end" type="float" min="-999" default="10" max="999" precision="2" _gui-text="End value">10</param>
                    <param name="slider_scale_label_rounding_float" type="int" min="0" max="3" default="1" precision="1" _gui-text="Rounding float">1</param>
                    <param name="slider_scale_label_reverse_order" type="boolean" _gui-text="Reverse order">false</param>
                    <param name="slider_scale_label_position" default="3" type="optiongroup" appearance="combo" gui-text="Label position">
                       <option value="1">Left</option>
                       <option value="2">Right</option>
                       <option value="3">Both</option>
                    </param>
                    <param name="slider_scale_label_font_size" type="float" min="1" max="10" default="1" precision="1" _gui-text="Label font size">1</param>
                    <param name="slider_scale_label_offset_tl" type="float" min="-10" max="30" default="1" precision="2" _gui-text="Label offset Left/Top">0</param>
                    <param name="slider_scale_label_offset_br" type="float" min="-10" max="30" default="1" precision="2" _gui-text="Label offset Right/Bottom">0</param>
                    <param name="slider_scale_label_offset_adj" type="float" min="-10" max="30" default="1" precision="2" _gui-text="Label fine positioning">0</param>
                    <param name="slider_scale_label_add_suffix" type="string" gui-text="Label suffix"></param>
                </vbox>
                <spacer/>
                <separator/>
                <spacer/>
                <vbox>
                    <label appearance="header">UTILITIES</label> 
                    <param name="slider_scale_utilities_color" type="color" appearance="colorbutton" _gui-text="Line color" default="#333333"></param>
                    <separator/>
                    <param name="slider_scale_utilities_add_drill_guide" type="boolean" _gui-text="Add drill guide">false</param>
                    <!-- <param name="slider_scale_utilities_drill_guide_type" type="optiongroup" _gui-text="Drill guide" appearance="combo">
                        <option value="2">Cross</option>
                        <option value="3">Dot</option>
                        <option value="4">Contour</option>
                    </param> -->
                    <param name="slider_scale_utilities_line_width" type="float" min="0" max="10" default="1" precision="2" _gui-text="Line width">0.2</param>
                    <separator/>
                    <param name="slider_scale_utilities_guide_round_edges" type="boolean" _gui-text="Round the edges">false</param>
                    <!-- <param name="slider_scale_utilities_guide_dimension" type="float" min="0" max="100" default="6.5" precision="2" _gui-text="Guide dimension">6.5</param> -->
                    <separator/>
                    <!-- <param name="slider_scale_linlog" type="optiongroup" _gui-text="Lin/log" appearance="radio">
                        <option value="1">Lin</option>
                        <option value="2">Log</option>
                    </param> -->
                </vbox>
            </hbox>
        </page>
